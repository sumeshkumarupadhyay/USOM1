@model IList<HistoryEntity>
@{
    ViewBag.Title = "history";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- subheader -->
<section id="subheader" data-speed="8" data-type="background" style="background-image: url(../../Content/web/assets/images/background/subheader-3.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>Our History</h1>
                <ul class="crumb">
                    <li><a href="/">Home</a></li>
                    <li class="sep">/</li>
                    <li>Our History</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<br />
<!-- subheader close -->
<!-- content begin -->
<div id="content" class="no-top no-bottom mt20 sm-mt0">
    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h2>History</h2>
                </div>
                <div class="col-md-6 offset-md-3">
                    <div class="timeline exp">
                        @foreach (var item in Model)
                        {
                            <div class="tl-block wow fadeInUp" data-wow-delay=".2s">
                                <div class="tl-time">
                                    <h4>@item.Year</h4>
                                </div>
                                <div class="tl-bar">
                                    <div class="tl-line"></div>
                                </div>
                                <div class="tl-message">
                                    <div class="tl-icon">&nbsp;</div>
                                    <div class="tl-main">
                                        <h4 class="id-color">
                                            @item.Title
                                        </h4>
                                        @Html.Raw(item.Description)
                                    </div>
                                </div>
                            </div>
                        }
