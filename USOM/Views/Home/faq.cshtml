@model IList<FaqEntity>
@{
    ViewBag.Title = "faq";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- subheader -->
<section id="subheader" data-speed="8" data-type="background" style="background-image: url(../../Content/web/assets/images/background/subheader-3.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>FAQ</h1>
                <ul class="crumb">
                    <li><a href="index.html">Home</a></li>
                    <li class="sep">/</li>
                    <li>FAQ</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- subheader close -->
<!-- content begin -->
<div id="content" class="no-top no-bottom">
    <section id="section-faq">
        <div class="container">
            <div class="row">
                <div class="col-md-3">
                    <h3 class="s2"><span class="id-color">Questions</span> </h3>
                </div>

                <div class="col-md-9">
                    @foreach (var item in Model)
                    {
                        <div class="expand-group">
                            <div class="expand">
                                <h4>@item.Title</h4>
                                <div class="hidden-content">
                                    @Html.Raw(item.Description)
                                </div>
                            </div>
                        </div>
                    }

                    </div>


            </div>
        </div>
    </section>
</div>