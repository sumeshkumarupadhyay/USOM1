        <a href="#" id="back-to-top"></a>
    </footer>
    <!-- footer close -->
    <!-- Javascript Files
    ================================================== -->
    <script src="~/content/web/assets/js/plugins.js"></script>
    <script src="~/content/web/assets/js/designesia.js"></script>

    <!-- SLIDER REVOLUTION SCRIPTS  -->
    <script type="text/javascript" src="~/content/web/assets/rs-plugin/js/jquery.themepunch.plugins.min.js"></script>
    <script type="text/javascript" src="~/content/web/assets/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script>
        window.onload = function () {
            location.reload(true); // true parameter indicates a hard reload
        };

        // Wait for the audio element to be fully loaded
        document.getElementById("my-audio").addEventListener("loadeddata", function () {
            handleAudio(); // Call the function when the audio is fully loaded
        });

        function handleAudio() {
            const audio = document.getElementById("my-audio");
            console.log("handleAudio function is called.");

            const sectionHero = document.getElementById("section-hero");
            console.log("sectionHero element:", sectionHero);

            if (!sectionHero) {
                audio.pause();
            }

            audio.currentTime = 0;
            setTimeout(function () {
                audio.pause();
            }, 60000);
        }
    </script>


    @RenderSection("Scripts", required: false)

</body>
</html>
