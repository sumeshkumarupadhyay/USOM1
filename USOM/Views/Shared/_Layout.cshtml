<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    @{ string image = ViewBag.MetaImage == null ? "/content/web/assets/images/logo.png" : ViewBag.MetaImage;}
    <link rel="shortcut icon" href="~/Content/web/assets/images/logog.png" type="image/x-icon">
    <title>@ViewBag.Title | Global Cancer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @{ string type = image.Split('.')[1];}
    <!--Basic meta details-->
    <meta name="description" content="@Html.Raw(ViewBag.MetaDescription)" />
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta itemprop="url" content="@Request.Url.Host" />
    <meta property="image:width" content="300">
    <meta property="image:height" content="200">
    <meta property="image" content="@image" />
    <meta property="image:type" content="image/@type" />



    <!--Begin Facebook share details-->
    <meta property="og:url" content="@Request.Url.AbsoluteUri" />
    <meta property="og:locale" content="hi_IN" />
    <meta property="og:type" content="article" />
    <meta property="og:site_name" content="Global Cancer" />
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:description" content="@Html.Raw(ViewBag.MetaDescription)" />
    <meta property="og:image" itemprop="image" content="@image" />
    <meta property="og:image:width" content="300" />
    <meta property="og:image:height" content="200" />
    <meta property="og:image:type" content="image/@type" />
    <meta property="fb:app_id" content="" />





    <!--End of Facebook share details-->
    <!--Begin twitter share details-->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="Jhar News">
    <meta name="twitter:url" content="@Request.Url.Host">
    <meta name="twitter:title" content="@ViewBag.Title">
    <meta name="twitter:description" content="@Html.Raw(ViewBag.MetaDescription)">
    <meta name="twitter:image" content="@image">
    <meta name="twitter:image:width" content="300" />
    <meta name="twitter:image:height" content="200" />
    <meta property="twitter:image:type" content="image/@type" />

    <!-- CSS Files
    ================================================== -->
    <link href="~/content/web/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bootstrap" />
    <link href="~/content/web/assets/css/bootstrap-grid.min.css" rel="stylesheet" type="text/css" id="bootstrap-grid" />
    <link href="~/content/web/assets/css/bootstrap-reboot.min.css" rel="stylesheet" type="text/css" id="bootstrap-reboot" />
    <link href="~/content/web/assets/css/plugins.css" rel="stylesheet" type="text/css">
    <link href="~/content/web/assets/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/content/web/assets/css/color.css" rel="stylesheet" type="text/css">
    <link href="~/content/web/assets/css/satyam.css" rel="stylesheet" type="text/css">

    <!-- custom background -->
    <link rel="stylesheet" href="~/content/web/assets/css/bg.css" type="text/css">

    <!-- color scheme -->
    <link rel="stylesheet" href="~/content/web/assets/css/colors/yellow.css" type="text/css" id="colors">

    <!-- revolution slider -->
    <link rel="stylesheet" href="~/content/web/assets/rs-plugin/css/settings.css" type="text/css">
    <link rel="stylesheet" href="~/content/web/assets/css/rev-settings.css" type="text/css">
</head>

<body id="homepage">
    <audio id="my-audio" autoplay>
        <source src="~/Content/web/assets/Video/Music.mp3" type="audio/mpeg">
    </audio>
    <div id="wrapper">

        <header class="transparent pl20 pr20">
